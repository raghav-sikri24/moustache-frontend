{
  "name": "moustache-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "yarn cache clean && rm -rf node_modules .next",
    "type-check": "tsc --noEmit",
